    "//老板胡汉三\n",
    "Boss huhansan = new Boss();\n",
    "\n",
    "//看股票的同事\n",
    "StockObserver tongshi1 = new StockObserver(\"魏关姹\",huhansan);\n",
    "//看NBA的同事\n",
    "NBAObserver tongshi2 = new NBAObserver(\"易管查\",huhansan);\n",
